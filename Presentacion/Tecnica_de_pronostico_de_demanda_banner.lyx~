\lang spanish
for cada período de análisis {mensual, quincenal,semanal}:
\end_layout

\begin_deeper
\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
for cada producto con sus instancias:
\end_layout

\begin_deeper
\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
establecer conjunto de entrenamiento;
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
establecer conjunto de testeo;
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
for cada algoritmo de clasificación:
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
   construir clasificador (conjunto de entrenamiento);
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
   evaluar clasificador (stratified k-folds cross-validation);
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
   obtener métricas de evaluación;
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
endfor;
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
criterios de línea de base (ZeroR, criterios del experto u otro);
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
seleccionar mejor clasificador (max(Kappa));
\end_layout

\begin_layout LyX-Code
\paragraph_spacing single

\size tiny
\lang spanish
guardar clasificador;
\end_layout

\end_deeper
\begin_layout LyX-Code
