\begin_layout Plain Layout
\begin_inset Flex Color Box
status collapsed

\begin_layout Plain Layout
\begin_inset Argument 1
status open

\begin_layout Plain Layout
title=Definición de KPI
\end_layout

\end_inset


\end_layout

\begin_layout Itemize

\lang spanish
Ticket Medio 
\shape italic
\size tiny
\color brown

\begin_inset Formula 
\begin{equation}
TM\,=\frac{Cantidad\,Vendida\,Periodo}{Total\,Tickets\,Periodo}\label{eq: Ticket Medio}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\lang spanish
Cifra de Ventas
\shape italic
\size tiny
\color brown

\begin_inset Formula 
\begin{equation}
CV\,=Precio*Cantidad\,Vendida\,Periodo\label{eq: Cifra de Ventas}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\lang spanish
Margen Comercial
\shape italic
\size tiny
\color brown

\begin_inset Formula 
\begin{equation}
MC\,=\frac{\left(Precio-Costo\right)*Cantidad\,Vendida\,Periodo}{Precio*Cantidad\,Vendida\,Periodo}*100\label{eq:Margen Comercial}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\lang spanish
Rotación de Stock
\shape italic
\size tiny
\color brown

\begin_inset Formula 
\begin{equation}
RS\,=\frac{Total\,Ventas\,Periodo}{\left(\frac{Stock\,Inicial\,Periodo-Stock\,Final\,Periodo}{2}\right)}\label{eq:Rotación Stock}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\lang spanish
Coeficiente de Rentabilidad
\shape italic
\size tiny
\color brown

\begin_inset Formula 
\begin{equation}
CR=\left(\left(Precio-Costo\right)*Cantidad\,Vendida\,Periodo\right)*RS\label{eq:Coeficiente Rentabilidad}
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize

\lang spanish
Cobertura de Stock
\shape italic
\size tiny
\color brown

\begin_inset Formula 
\begin{equation}
CS\,=\frac{Stock\,Actual\,Periodo}{Promedio\,Cantidad\,Venta\,Ultimos\,N\,Periodos}\label{eq:Cobertura Stock}
\end{equation}

\end_inset


\end_layout

\end_inset


\end_layout

