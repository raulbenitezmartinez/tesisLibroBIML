\begin_deeper
\begin_layout Itemize
El método de pronóstico del último valor.
\end_layout

\begin_layout Standard

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=último\;valor
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
El método de pronóstico por promedios.
\end_layout

\begin_layout Standard

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=promedio\;de\;todos\;los\;valores\;hasta\;la\;fecha
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
El método de pronóstico de promedio móvil.
\end_layout

\begin_layout Standard

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=promedio\;de\;los\;últimos\;n\;valores
\end{equation}

\end_inset


\color inherit
donde:
\end_layout

\begin_layout Standard
\align center

\size tiny
\color brown
\begin_inset Formula $n=número\;de\;periodos\;más\;recientes$
\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Frame
\begin_inset Argument 4
status open

\begin_layout Plain Layout
Pronósticos de series de tiempo
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize
El método de pronóstico por suavizamiento exponencial.
\end_layout

\begin_deeper
\begin_layout Standard

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=\alpha*\left(último\;valor\right)+\left(1\text{-}\alpha\right)*\left(último\;pronóstico\right)
\end{equation}

\end_inset


\color inherit
donde:
\end_layout

\begin_layout Standard
\align center

\size tiny
\color brown
\begin_inset Formula $\alpha$
\end_inset


\color inherit
 es una constante entre 0 y 1 llamada 
\begin_inset Quotes eld
\end_inset

constante de suavizamiento
\begin_inset Quotes erd
\end_inset

.
 
\end_layout

\end_deeper
\begin_layout Itemize
\noindent
El método de suavizamiento exponencial con tendencia.
\end_layout

\begin_deeper
\begin_layout Standard
\noindent
\align left

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=\alpha*\left(último\;valor\right)+\left(1\text{-}\alpha\right)*\left(último\;pronóstico\right)+tendencia\;estimada
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\align block

\shape italic
\size tiny
donde:
\color brown

\begin_inset Formula 
\begin{equation}
tendencia\;estimada=\beta*\left(última\;tendencia\right)*\left(1\text{-}\beta\right)*\left(estimación\;anterior\right)
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\begin{array}{c}
última\;tendencia\:=\:\alpha*\left(último\;valor-penúltimo\;valor\right)\,+\\
\qquad\qquad\qquad\qquad\qquad\qquad\qquad\left(1\text{-}\alpha\right)*\left(último\;pronóstico-penúltimo\;pronóstico\right)
\end{array}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\noindent
\align left

\size tiny
con 
\color brown

\begin_inset Formula $\beta$
\end_inset


\color inherit
 (beta) es una constante de suavizamiento de tendencia entre 0 y 1.
 La elección del valor y rango de 
\color brown

\begin_inset Formula $\beta$
\end_inset


\color inherit
 tienen igual significado que 
\color brown

\begin_inset Formula $\alpha$
\end_inset


\color inherit
.
\end_layout

\end_deeper
