\begin_deeper
\begin_layout Itemize
El método de pronóstico del último valor.
\end_layout

\begin_layout Standard

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=último\;valor
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
El método de pronóstico por promedios.
\end_layout

\begin_layout Standard

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=promedio\;de\;todos\;los\;valores\;hasta\;la\;fecha
\end{equation}

\end_inset


\end_layout

\begin_layout Itemize
El método de pronóstico de promedio móvil.
\end_layout

\begin_layout Standard

\shape italic
\size tiny
\color brown
\begin_inset Formula 
\begin{equation}
Pronóstico=promedio\;de\;los\;últimos\;n\;valores
\end{equation}

\end_inset


\color inherit
donde:
\end_layout

\begin_layout Standard
\align center

\size tiny
\color brown
\begin_inset Formula $n=número\;de\;periodos\;más\;recientes$
\end_inset


\end_layout

\end_deeper
\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

